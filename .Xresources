URxvt.font: xft:Source Code Pro:style=Regular:size=14

!Keeping it here because commmand can be used while a program runs
!URxvt.keysym.Control-l: echo -ne '\\033c'\n
!URxvt.keysym.Control-l: command:\033c

!Not using Perl extensions so disable them to improve security and speed
URxvt.perl-ext:
URxvt.perl-ext-common:

!Remove the scrollbar
!URxvt.scrollBar: false

! Transparent terminal
!URxvt.transparent:   true
!URxvt.shading:       20

!Colors - Solarized Dark
! special
*.foreground:   #93a1a1
*.background:   #002b36
*.cursorColor:  #93a1a1

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

